<h1>Sign Up For The Odin Facebook</h1>

<%= form_for @user do |f| %>
	<%= f.label(:first_name, "First Name:") %>
	<%= f.text_field(:first_name) %>
	<%= f.label(:last_name, "Last Name:") %>
	<%= f.text_field(:last_name) %>
	<%= f.label(:email, "Email:") %>
	<%= f.text_field(:email) %>
	<%= f.submit("Sign Up") %>
<% end %>
